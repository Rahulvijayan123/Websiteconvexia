{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "cagr": {
      "type": "string",
      "description": "Compound Annual Growth Rate as percentage or range"
    },
    "marketSize": {
      "type": "string",
      "description": "Current and projected market size in billions"
    },
    "directCompetitors": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "Names of direct competitors"
    },
    "prvEligibility": {
      "oneOf": [
        {"type": "string"},
        {"type": "number"}
      ],
      "description": "Priority Review Voucher eligibility"
    },
    "nationalPriority": {
      "type": "string",
      "description": "Public health priority tier"
    },
    "reviewTimelineMonths": {
      "oneOf": [
        {"type": "string"},
        {"type": "number"}
      ],
      "description": "Expected FDA review timeline in months"
    },
    "peakRevenue2030": {
      "type": "string",
      "description": "Forecasted peak revenue by 2030 in billions"
    },
    "total10YearRevenue": {
      "type": "string",
      "description": "Estimated total revenue over 10 years"
    },
    "peakMarketShare2030": {
      "type": "string",
      "description": "Projected market share by 2030 as percentage"
    },
    "peakPatients2030": {
      "type": "string",
      "description": "Estimated treated patient population by 2030"
    },
    "avgSellingPrice": {
      "type": "string",
      "description": "Average selling price per patient in dollars"
    },
    "persistenceRate": {
      "type": "string",
      "description": "12-month treatment persistence rate as percentage"
    },
    "treatmentDuration": {
      "type": "string",
      "description": "Median treatment duration in months"
    },
    "geographicSplit": {
      "type": "string",
      "description": "Revenue split between US and ex-US markets at peak"
    },
    "competitorPricing": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "drugName": {"type": "string"},
          "annualPrice": {"type": "string"},
          "indication": {"type": "string"},
          "accessLevel": {"type": "string"},
          "rationale": {"type": "string"}
        },
        "required": ["drugName", "annualPrice", "indication", "accessLevel", "rationale"]
      },
      "description": "Pricing and access data for direct competitors"
    },
    "pricingScenarios": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "scenario": {"type": "string"},
          "usPrice": {"type": "string"},
          "exUsPrice": {"type": "string"},
          "grossToNet": {"type": "string"},
          "copay": {"type": "string"},
          "access": {"type": "string"},
          "genericYear": {"type": "string"},
          "lossOfExclusivity": {"type": "string"},
          "rationale": {"type": "string"}
        },
        "required": ["scenario", "usPrice", "exUsPrice", "grossToNet", "copay", "access", "genericYear", "lossOfExclusivity", "rationale"]
      },
      "description": "Real pricing strategy scenarios with market-based values"
    },
    "strategicTailwindData": {
      "type": "object",
      "properties": {
        "fdaDesignations": {
          "type": "object",
          "properties": {
            "breakthroughTherapy": {"type": "string"},
            "fastTrack": {"type": "string"},
            "orphanDrug": {"type": "string"},
            "priorityReview": {"type": "string"},
            "rationale": {"type": "string"}
          },
          "required": ["breakthroughTherapy", "fastTrack", "orphanDrug", "priorityReview", "rationale"]
        },
        "guidanceDocuments": {
          "type": "object",
          "properties": {
            "oncologyEndpoints": {"type": "string"},
            "biomarkerStrategy": {"type": "string"},
            "realWorldEvidence": {"type": "string"},
            "patientReportedOutcomes": {"type": "string"},
            "rationale": {"type": "string"}
          },
          "required": ["oncologyEndpoints", "biomarkerStrategy", "realWorldEvidence", "patientReportedOutcomes", "rationale"]
        },
        "policyIncentives": {
          "type": "object",
          "properties": {
            "arpaHFunding": {"type": "string"},
            "medicareInnovation": {"type": "string"},
            "iraExclusions": {"type": "string"},
            "taxCredits": {"type": "string"},
            "rationale": {"type": "string"}
          },
          "required": ["arpaHFunding", "medicareInnovation", "iraExclusions", "taxCredits", "rationale"]
        },
        "advocacyActivity": {
          "type": "object",
          "properties": {
            "patientOrganizations": {"type": "string"},
            "kolSupport": {"type": "string"},
            "medicalSocieties": {"type": "string"},
            "congressionalInterest": {"type": "string"},
            "rationale": {"type": "string"}
          },
          "required": ["patientOrganizations", "kolSupport", "medicalSocieties", "congressionalInterest", "rationale"]
        },
        "marketPrecedent": {
          "type": "object",
          "properties": {
            "similarApprovals": {"type": "string"},
            "pricingPrecedent": {"type": "string"},
            "accessPatterns": {"type": "string"},
            "competitiveResponse": {"type": "string"},
            "rationale": {"type": "string"}
          },
          "required": ["similarApprovals", "pricingPrecedent", "accessPatterns", "competitiveResponse", "rationale"]
        }
      },
      "required": ["fdaDesignations", "guidanceDocuments", "policyIncentives", "advocacyActivity", "marketPrecedent"]
    },
    "dealActivity": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "assetName": {"type": "string"},
          "buyer": {"type": "string"},
          "developmentStage": {"type": "string"},
          "dealPrice": {"type": "string"},
          "dealDate": {"type": "string"},
          "rationale": {"type": "string"}
        },
        "required": ["assetName", "buyer", "developmentStage", "dealPrice", "dealDate", "rationale"]
      },
      "description": "Recent M&A/licensing deals"
    },
    "pipelineAnalysis": {
      "type": "object",
      "properties": {
        "crowdingPercent": {
          "type": "string",
          "description": "Pipeline density as percentage"
        },
        "competitiveThreats": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "List of competitive threats"
        },
        "strategicFitRank": {
          "type": "string",
          "description": "Strategic fit rank 0-100"
        }
      },
      "required": ["crowdingPercent", "competitiveThreats", "strategicFitRank"]
    }
  },
  "required": [
    "cagr",
    "marketSize", 
    "directCompetitors",
    "prvEligibility",
    "nationalPriority",
    "reviewTimelineMonths",
    "peakRevenue2030",
    "total10YearRevenue",
    "peakMarketShare2030",
    "peakPatients2030",
    "avgSellingPrice",
    "persistenceRate",
    "treatmentDuration",
    "geographicSplit",
    "competitorPricing",
    "pricingScenarios",
    "strategicTailwindData",
    "dealActivity",
    "pipelineAnalysis"
  ],
  "additionalProperties": false
} 